#include<stdio.h>
#include<stdint.h>

//function defs
void waitKey(void);

struct carModel{
	unsigned int carNumber;
	uint32_t carPrice;
	uint16_t carMaxSpeed;
	float carWeight;
};

int main()
{
	struct carModel carMercedes, carVolvo, carToyota;

	carMercedes = {2021, 15000, 220, 1330};
	carVolvo = {4031, 35000, 160, 1900.96};

	printf("%d", carMercedes);

	waitKey();
	return 0;
}

void waitKey(){
	//Exit Section with wait KEY
	printf("\nPress 'Enter' key to EXIT..");
	while(getchar() != '\n'){};
	getchar();
}

